  "license": "MIT",
  "private": false,
  "dependencies": {
    "google-u2f-api.js": "^1.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "laravel-mix": "^2.0.0",
    "webpack-bundle-analyzer": "^2.10.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
